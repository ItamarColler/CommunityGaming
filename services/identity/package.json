{
  "name": "@community-gaming/identity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@community-gaming/types": "workspace:*",
    "@community-gaming/utils": "workspace:*",
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1",
    "fastify": "^4.25.2",
    "pino": "^8.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@community-gaming/config": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.1"
  }
}
